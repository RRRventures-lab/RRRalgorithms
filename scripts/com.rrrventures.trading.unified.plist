<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.rrrventures.trading.unified</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/Volumes/Lexar/RRRVentures/RRRalgorithms/venv/bin/python</string>
        <string>/Volumes/Lexar/RRRVentures/RRRalgorithms/src/main_unified.py</string>
        <string>--mode</string>
        <string>paper</string>
        <string>--log-level</string>
        <string>INFO</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Volumes/Lexar/RRRVentures/RRRalgorithms</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>

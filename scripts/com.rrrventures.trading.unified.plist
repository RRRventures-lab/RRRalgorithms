    <dict>
        <key>TRADING_HOME</key>
        <string>/Volumes/Lexar/RRRVentures/RRRalgorithms</string>
        <key>DATABASE_TYPE</key>
        <string>sqlite</string>
        <key>DATABASE_PATH</key>
        <string>/Volumes/Lexar/RRRVentures/RRRalgorithms/data/db/trading.db</string>
        <key>PYTHONPATH</key>
        <string>/Volumes/Lexar/RRRVentures/RRRalgorithms/src</string>
        <key>SQLITE_TMPDIR</key>
        <string>/tmp</string>
    </dict>
    
    <key>SoftResourceLimits</key>
    <dict>
        <key>NumberOfFiles</key>
        <integer>4096</integer>
    </dict>
    
    <key>Nice</key>
    <integer>-5</integer>
    
</dict>
</plist>


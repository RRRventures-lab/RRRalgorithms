{
  "dashboard": {
    "id": null,
    "uid": "rrr-ml-model",
    "title": "RRR Trading - ML Model Performance",
    "tags": ["rrr-trading", "ml", "neural-network"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
        "type": "stat",
        "title": "Model Accuracy (24h)",
        "targets": [
          {
            "expr": "ml_prediction_accuracy",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "orange"},
                {"value": 0.55, "color": "yellow"},
                {"value": 0.6, "color": "green"},
                {"value": 0.7, "color": "dark-green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
        "type": "stat",
        "title": "Average Confidence",
        "targets": [
          {
            "expr": "avg_over_time(ml_prediction_confidence[10m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.5, "color": "yellow"},
                {"value": 0.7, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 0},
        "type": "stat",
        "title": "GPU Utilization",
        "targets": [
          {
            "expr": "ml_gpu_utilization_percent",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 20, "color": "yellow"},
                {"value": 50, "color": "green"},
                {"value": 90, "color": "orange"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 0},
        "type": "stat",
        "title": "Inference Requests/sec",
        "targets": [
          {
            "expr": "rate(ml_inference_requests_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "textMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 6},
        "type": "graph",
        "title": "Model Accuracy Over Time",
        "targets": [
          {
            "expr": "ml_prediction_accuracy",
            "legendFormat": "Accuracy",
            "refId": "A"
          },
          {
            "expr": "0.55",
            "legendFormat": "Target Accuracy",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Accuracy", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "seriesOverrides": [
          {
            "alias": "Target Accuracy",
            "color": "yellow",
            "fill": 0,
            "linewidth": 1,
            "dashes": true
          }
        ]
      },
      {
        "id": 6,
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 6},
        "type": "graph",
        "title": "Inference Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(ml_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(ml_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(ml_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency (seconds)", "min": 0},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2,
        "legend": {"show": true, "alignAsTable": true}
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "type": "graph",
        "title": "GPU Memory Usage",
        "targets": [
          {
            "expr": "ml_gpu_memory_used_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Used",
            "refId": "A"
          },
          {
            "expr": "ml_gpu_memory_total_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Total",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "decgbytes", "label": "Memory (GB)"},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 2,
        "linewidth": 2,
        "seriesOverrides": [
          {
            "alias": "Total",
            "color": "gray",
            "fill": 0,
            "linewidth": 1,
            "dashes": true
          }
        ]
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "type": "graph",
        "title": "Prediction Confidence Distribution",
        "targets": [
          {
            "expr": "histogram_quantile(0.10, ml_prediction_confidence_bucket)",
            "legendFormat": "p10",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, ml_prediction_confidence_bucket)",
            "legendFormat": "p50 (median)",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.90, ml_prediction_confidence_bucket)",
            "legendFormat": "p90",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Confidence", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24},
        "type": "piechart",
        "title": "Prediction Direction Distribution",
        "targets": [
          {
            "expr": "sum by(direction) (ml_predictions_total)",
            "legendFormat": "{{direction}}",
            "refId": "A"
          }
        ],
        "options": {
          "pieType": "donut",
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24},
        "type": "graph",
        "title": "Model Training Loss",
        "targets": [
          {
            "expr": "ml_training_loss",
            "legendFormat": "Training Loss",
            "refId": "A"
          },
          {
            "expr": "ml_validation_loss",
            "legendFormat": "Validation Loss",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Loss", "logBase": 10},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 1,
        "linewidth": 2
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24},
        "type": "graph",
        "title": "Feature Importance",
        "targets": [
          {
            "expr": "ml_feature_importance",
            "legendFormat": "{{feature}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Importance"},
          {"format": "short"}
        ],
        "bars": true,
        "lines": false,
        "legend": {"show": true, "alignAsTable": true, "rightSide": true}
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32},
        "type": "graph",
        "title": "Transformer Attention Weights (Layer 1, Head 1)",
        "targets": [
          {
            "expr": "ml_attention_weights{layer=\"1\",head=\"1\"}",
            "legendFormat": "Position {{position}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Attention Weight", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "lines": true,
        "fill": 0,
        "linewidth": 1
      }
    ],
    "time": {"from": "now-6h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["30s", "1m", "5m", "15m"]
    }
  }
}
